    <link rel="stylesheet" href="/assets/css/style.css">
    <style>
        .file-layout {
            display: flex;
            gap: 32px;
            margin-top: 24px;
            height: calc(100vh - 140px);
        }

        .file-menu {
            width: 260px;
            flex-shrink: 0;
            background: var(--darkestdark);
            border: 1px solid var(--darkhover);
            border-radius: 10px;
            padding: 16px 14px;
        }

        .file-menu-section {
            margin-top: 12px;
        }

        .file-menu-section:first-of-type {
            margin-top: 0;
        }

        .file-menu-section-label {
            font-size: 11px;
            text-transform: uppercase;
            letter-spacing: 0.04em;
            color: var(--light);
            margin-bottom: 6px;
        }

        .file-menu-list {
            display: flex;
            flex-direction: column;
            gap: 8px;
        }

        .file-menu-item {
            display: flex;
            align-items: center;
            justify-content: space-between;
            padding: 8px 10px;
            border-radius: 6px;
            background: var(--darkestdark);
            border: 1px solid var(--darkhover);
            color: var(--light);
            font-size: 13px;
        }

        .file-menu-item:hover {
            background: var(--darkhover);
        }

        .file-menu-name {
            overflow: hidden;
            text-overflow: ellipsis;
            white-space: nowrap;
        }

        .file-menu-empty {
            font-size: 12px;
            opacity: 0.7;
            margin-top: 2px;
        }

        .file-editor-container {
            flex: 1;
            display: flex;
            flex-direction: column;
            min-width: 0;
        }

        .editor-form {
            display: flex;
            flex-direction: column;
            flex: 1;
            min-height: 0;
        }

        .editor {
            flex: 1;
            width: 100%;
            box-sizing: border-box;
            resize: none;
        }

        .editor-actions {
            display: flex;
            justify-content: flex-end;
            margin-top: 12px;
            gap: 8px;
        }

        .file-empty-state {
            flex: 1;
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            text-align: center;
            max-width: 320px;
            margin: 0 auto;
        }
    </style>
</head>

